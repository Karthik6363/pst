    transform: translateX(0);
}

.k-fx-tile.k-fx-right.k-fx-start .k-fx-next {
    transform: translateX(-100%);
}

.k-fx-tile.k-fx-right.k-fx-end .k-fx-current {
    transform: translateX(100%);
}

